<mxfile host="app.diagrams.net">
  <diagram id="erd1" name="ERD Inventario LIA">
    <mxGraphModel>
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- ELEMENTOS -->
        <mxCell id="elementos" value="elementos\n--------------\nPK nro_lia\nnro_unsj\ntipo\ndescripcion\ncantidad"
          style="shape=swimlane;childLayout=stackLayout;horizontal=1;startSize=26;rounded=0;fontSize=14;"
          vertex="1" parent="1">
          <mxGeometry x="70" y="70" width="200" height="180" as="geometry"/>
        </mxCell>

        <!-- USUARIOS -->
        <mxCell id="usuarios" value="usuarios\n--------------\nPK id\nnombre\npassword"
          style="shape=swimlane;childLayout=stackLayout;horizontal=1;startSize=26;fontSize=14;"
          vertex="1" parent="1">
          <mxGeometry x="400" y="70" width="200" height="150" as="geometry"/>
        </mxCell>

        <!-- ROLES -->
        <mxCell id="roles" value="roles\n--------------\nPK id\nrol"
          style="shape=swimlane;childLayout=stackLayout;horizontal=1;startSize=26;fontSize=14;"
          vertex="1" parent="1">
          <mxGeometry x="400" y="300" width="200" height="120" as="geometry"/>
        </mxCell>

        <!-- USUARIO_ROLES -->
        <mxCell id="usuario_roles" value="usuario_roles\n--------------\nPK user_id\nPK rol_id"
          style="shape=swimlane;childLayout=stackLayout;horizontal=1;startSize=26;fontSize=14;"
          vertex="1" parent="1">
          <mxGeometry x="700" y="190" width="210" height="120" as="geometry"/>
        </mxCell>

        <!-- MOVIMIENTOS -->
        <mxCell id="movimientos" value="movimientos\n--------------\nPK id\nnro_lia\nnro_unsj\nuser_id\nestado\nubicacion\nfecha\ncomentario"
          style="shape=swimlane;childLayout=stackLayout;horizontal=1;startSize=26;fontSize=14;"
          vertex="1" parent="1">
          <mxGeometry x="70" y="330" width="260" height="240" as="geometry"/>
        </mxCell>

        <!-- RELACIONES (Crow's Foot arrows) -->

        <!-- elementos 1:N movimientos -->
        <mxCell id="rel_elem_mov" edge="1" parent="1"
          style="endArrow=crowFoot;startArrow=none;">
          <mxGeometry relative="1" as="geometry"/>
          <mxCell source="elementos" target="movimientos"/>
        </mxCell>

        <!-- usuarios 1:N movimientos -->
        <mxCell id="rel_user_mov" edge="1" parent="1"
          style="endArrow=crowFoot;">
          <mxGeometry relative="1" as="geometry"/>
          <mxCell source="usuarios" target="movimientos"/>
        </mxCell>

        <!-- usuarios 1:N usuario_roles -->
        <mxCell id="rel_user_ur" edge="1" parent="1"
          style="endArrow=crowFoot;">
          <mxGeometry relative="1" as="geometry"/>
          <mxCell source="usuarios" target="usuario_roles"/>
        </mxCell>

        <!-- roles 1:N usuario_roles -->
        <mxCell id="rel_rol_ur" edge="1" parent="1"
          style="endArrow=crowFoot;">
          <mxGeometry relative="1" as="geometry"/>
          <mxCell source="roles" target="usuario_roles"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>